{
  "extends": "airbnb",
  "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "jasmine": true,
      "amd": "true",
      "es6": true
  },
  "globals": {
    "nmsweb": true
  },
  "ecmaVersion": 6,
  "rules": {
      "no-var": 0,
      "indent": 2,
      "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
      "prefer-arrow-callback": 1,
      "max-len": [1, 100, 4, {"ignoreUrls": true}],
      "comma-dangle": [2, "only-multiline"]
  }
}
